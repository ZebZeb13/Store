{
  "name": "DeviStore",
  "version": "1.0.0",
  "description": "Database",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "##prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typedoc": "typedoc --mode modules -module commonjs --exclude **/*.spec.ts --out docs/typedoc src",
    "compodoc": "npx compodoc -p ./tsconfig.json -s -o -w --port 5555"
  },
  "dependencies": {
    "@nestjs/common": "6.8.5",
    "@nestjs/core": "6.8.5",
    "@nestjs/graphql": "^6.5.3",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "6.8.5",
    "@nestjs/platform-fastify": "^6.10.14",
    "@nestjs/typeorm": "6.2.0",
    "@types/jest": "^24.0.23",
    "apollo-server-core": "^2.9.9",
    "apollo-server-express": "^2.9.9",
    "apollo-server-fastify": "^2.9.16",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "dateformat": "^3.0.3",
    "dotenv": "^8.2.0",
    "mysql": "2.17.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.0",
    "rxjs": "6.5.3",
    "simple-node-logger": "^18.12.23",
    "typeorm": "0.2.20",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.2.1"
  },
  "devDependencies": {
    "@nestjs/cli": "6.11.2",
    "@nestjs/schematics": "6.7.0",
    "@nestjs/testing": "6.8.5",
    "@types/express": "4.17.1",
    "@types/node": "7.10.9",
    "@types/supertest": "2.0.8",
    "@types/ws": "6.0.3",
    "jest": "24.9.0",
    "prettier": "1.18.2",
    "supertest": "4.0.2",
    "ts-jest": "24.1.0",
    "ts-loader": "6.2.1",
    "ts-node": "8.4.1",
    "tsconfig-paths": "3.9.0",
    "tslint": "5.20.0",
    "typescript": "3.6.4"
  },
  "main": "./src/main.ts",
  "repository": "git@github.com:ZebZeb13/DeviStore.git",
  "author": "ZebZeb13 <chamayou.guilhem13@gmail.com>"
}
